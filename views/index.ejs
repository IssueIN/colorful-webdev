<form action="/products/results" method="GET">
  <div>
    <input type="text" name="search" value="<%= searchOptions.search %>" placeholder="Enter Part#">
  </div>
  <button type="submit">Search</button>
</form>

<h2>Recently Added</h2>
<div>
  <% productsRecent.forEach(product => { %>
    <div><%= product.partNumber %></div>
  <% }) %>
</div>

<h2>Manufacturers</h2>
<div>
  <% manufacturers.forEach(manufacturer => { %>
    <div><%= manufacturer.name%></div>
  <% }) %>
</div>
