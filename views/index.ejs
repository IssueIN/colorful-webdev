<div class="index-page-container">
  <div class="main-carousel">
    <%- include('./partials/carousel', {
      pathToSlideFile: '_main_carousel',
      slidesCount: carouselImgCount,
      buttonStyle: 'main-carousel__button'
    }) %>
  </div>
  
  <h2>Recently Added</h2>
  
  <div class="recentProduct-carousel">
    <%- include('./partials/carousel', {
      pathToSlideFile: '_recentProducts_carousel',
      slidesCount: 5,
      buttonStyle: 'recentProducts-carousel__button',
    }) %>
  </div>
  
  <div>
    <% recentProducts.forEach(product => { %>
      <div><%= product.partNumber %></div>
    <% }) %>
  </div>
  
  <h2>Manufacturers</h2>
  <div>
    <% manufacturers.forEach(manufacturer => { %>
      <div><%= manufacturer.name%></div>
    <% }) %>
  </div>
</div>
<!-- Carousel Part -->

<script src="/javascripts/carousel.js"></script>
