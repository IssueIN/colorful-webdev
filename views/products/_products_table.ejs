<table>
  <tr>
    <th class="product-table__first-col"><%= t("productsTableFirstCol") %></th>
    <th class="product-table__second-col"><%= t("quantityAvaliable") %></th>
    <th><%= t("price") %></th>
    <th><%= t("packageType") %></th>
    <th><%= t("packaging") %></th>
    <th><%= t("datasheet") %></th>
  </tr>
  <% products.forEach(product => { %>
    <tr>
      <td>
        <div class="product-info-container">
          <div class="product-img-container">
            <img src="/uploads/productImg-placeholder.png" height="48">
          </div>
          <div>
            <%= product.partNumber %><br>
            <%= product.manufacturer.name %>
          </div>
        </div>
      </td>
      <td><%= product.stock %></td>
      <td><% product.price.forEach(pricelevel => { %>
        <%= pricelevel.qty %> : <%= pricelevel.price %><br>
      <%}) %>
      </td>
      <td><%= product.packageCase[lang] %></td>
      <td><%= product.packaging[lang] %></td>
      <td>place for datasheet</td>
    </tr>
  <% }) %>
</table>